{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "TxEditor",
  "version": "0.1.0",
  "identifier": "com.txeditor.app",
  "build": {
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build",
    "devUrl": "http://localhost:1420",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "TxEditor",
        "width": 1280,
        "height": 720,
        "resizable": true
      }
    ],
    "security": {
      "csp": null
    }
  }
}

